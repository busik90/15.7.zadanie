  "description": "Przekształcenie stopera na składnię ES6",
  "main": "script.js",
  "scripts": {
    "watch": "npm-run-all -p watch-*",
    "watch-script.js": "babel script.js --watch --out-file script-compiled.js",
    "watch-App.js": "babel components/App.js --watch --out-file components/compiled/App.js",
    "watch-Controls.js": "babel components/Controls.js --watch --out-file components/compiled/Controls.js",
    "watch-Stopwatch.js": "babel components/Stopwatch.js --watch --out-file components/compiled/Stopwatch.js",
    "watch-ScoresTable.js": "babel components/ScoresTable.js --watch --out-file components/compiled/ScoresTable.js",
    "watch-ClearListButton.js": "babel components/ClearListButton.js --watch --out-file components/compiled/ClearListButton.js"
  },
  "keywords": [
    "Babel",
    "ES6",
    "Classes",
    "React"
  ],
