    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "globstar": "^1.0.0",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {},
